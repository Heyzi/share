[Unit]
Description=GitLab Activity Exporter
After=network.target

[Service]
Type=simple
User=gitlab-exporter
Group=gitlab-exporter
ExecStart=/usr/local/bin/gitlab-activity-exporter
Restart=always
RestartSec=10s

# Безопасность
NoNewPrivileges=true
ProtectSystem=full
ProtectHome=true
PrivateTmp=true

# Limits
LimitNOFILE=4096

[Install]
WantedBy=multi-user.target
